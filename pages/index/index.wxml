<!-- This is our View -->
<scroll-view class='mainScrollClass' scroll-y style="width: 100%" bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}"> 
  <view class='headerClass'>全部信息</view>
  <view wx:for="{{myJobList}}" wx:key="{{item}}">
    <view class='jobListClass'>{{item}}</view>
  </view>
</scroll-view>

